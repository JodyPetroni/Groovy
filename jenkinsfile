node {
    stage('SCM') {
        echo 'GETTING CODE FROM SORUCE CONTROL....'
        git branch:'${branch}', url: 'https://github.com/JodyPetroni/Groovy.git'
    }
    stage('Build') {
        echo 'Building....'
        sh '/usr/bin/dotnet --version'
        sh '/usr/bin/dotnet build src/console.csproj'
    }
    stage('Test') {
        echo 'Testing....'
    }
    stage('Deploy') {
        echo 'Deploying....'
    }
}
